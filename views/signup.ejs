<body>
    <form action="/sign" class="form-group" method="POST">
     
          <!--<label for="">username</label>-->
        <input type="text" name="username"required class="form-control" placeholder="Username" id="">
        <br>
        <!-- <label for="">Email</label>-->
        <input type="email" name="email" placeholder="Email" class="form-control" required id="">
        <br>
        <!-- <label for="">Password</label>-->

        <input type="password" name="password" placeholder="Password" class="form-control" required id="">
        <br>
        <!-- <label for="">Confirm Password</label>-->
        <input type="password" name="confirmpassword" class="form-control" placeholder="Confirm Password" required id="">
       <p style="color:whitesmoke"><%=errorpass%></p>
        <br>
<center>   
  <label for="">Mail</label><br>
    <input type="radio" name="gender"    placeholder="Mail" value="Mail">
    <br>
     <label for="" >Femail</label><br>
    <input type="radio" name="gender"  placeholder="Femail"  value="Femail">
    <br>
    <p style="color:whitesmoke"><%=errorfound%></p>
